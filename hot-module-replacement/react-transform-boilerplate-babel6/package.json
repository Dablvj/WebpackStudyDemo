{
  "name": "react-transform-boilerplate-babel6",
  "version": "0.1.0",
  "description": "A Webpack boilerplate with hot reloading React components, and error handling on module and component level —— By babel6.",
  "scripts": {
    "clean": "rimraf dist",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "start": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zhbhun/WebpackStudyDemo.git"
  },
  "keywords": [
    "babel",
    "babel6",
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "webpack"
  ],
  "author": "zhbhun",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zhbhun/WebpackStudyDemo/issues"
  },
  "homepage": "https://github.com/zhbhun/WebpackStudyDemo",
  "devDependencies": {
    "babel-core": "^6.18.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "cross-env": "^3.1.3",
    "eventsource-polyfill": "~0.9.6",
    "express": "^4.13.3",
    "rimraf": "^2.4.3",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.0"
  },
  "dependencies": {
    "react": "^15.3.2",
    "react-dom": "^15.3.2"
  }
}
